{
  "request": {
    "method": "POST",
    "url": "/api/v1/tracking",
    "headers": {
      "Accept": {
        "equalTo": "*/*"
      }
    },
    "bodyPatterns": [
      {
        "matchesJsonPath": {
          "expression": "$.number",
          "contains": "LA144130537CN"
        }
      }
    ]
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json"
    },
    "bodyFileName": "belpost-response.json"
  }
}
